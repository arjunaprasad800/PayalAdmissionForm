class Menu:
    def form(self,name,age,group,registrationno,parentsreference):
        self.name= name
        name= input()
        if self.name == True:
            print("Go to the next field")
        else:
            return Menu
        self.age = age
        age = int(input())
        if self.age ==True:
            print("Go to the next field")
        else:
            return Menu
        self.group= group
        group = input()
        if self.group==True:
            print("Go to the next field")
        else:
            return Menu
        self.registrationno = registrationno
        registrationno= int(input())
        if self.registrationno==True:
            print("Go to the next field")
        else:
            return Menu
        self.parentsreference = parentsreference
        parentsreference= list[1,2]
        parentsreference = int(input())
        parentsreference = input()
        if self.parentsreference==list:
            print("Go to the next field")
        else:
            return Menu
        

        
