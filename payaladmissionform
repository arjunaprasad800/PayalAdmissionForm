class Subsection:
    def subsections():
        def __init__(self,Photo,format,captionno,nationalidno,admissionpreference):
            self.Photo = Photo
            Photo == ("#caption&format")
            captionno = Photo
            captionno =int(input())
            format= input()
            Photo= captionno+format
            if self.Photo== Photo:
                print("Identification Done")
            else:
                return Photo
            self.nationalidno= nationalidno
            nationalidno= int(input().split())
            if self.nationalidno == True:
                print("Request Done")
            elif self.nationalidno == False:
                return Photo
                if Photo== True:
                    print("Verify Again") 
                else:
                    return Subsection
            self.admissionpreference= admissionpreference
            admissionpreference1= "Science"
            admissionpreference2= "Arts"
            admissionpreference= admissionpreference1 + admissionpreference2 
            if self.admissionpreference == admissionpreference1:
                print("Best wishes for choosing Science")
            else:
                print("Best wishes for choosing Arts") 


